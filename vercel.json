{
  "version": 2,
  "builds": [
    {
      "src": "frontend/public/**",
      "use": "@vercel/static"
    },
    {
      "src": "backend/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "frontend/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/public/$1"
    }
  ]
}